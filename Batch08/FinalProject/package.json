{
  "name": "grabservices",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "rm -rf ./android/app/build && rm -rf ./node_modules/react-native-onesignal/android/build && rm -rf ./node_modules/react-native-maps/android/build && react-native run-android",
    "build:apk": "cd android && ./gradlew assembleRelease",
    "build:aab": "cd android && ./gradlew bundleRelease",
    "test:android": "npx react-native run-android --variant=release",
    "clean:apk": "cd android && ./gradlew cleanBuildCache",
    "uninstall": "adb uninstall vn.softech.grabservices",
    "codepush:android": "appcenter codepush release-react -a ngothanhtung.it-gmail.com/Grab-Services-Android -d Production -m",
    "codepush:ios": "appcenter codepush release-react -a ngothanhtung.it-gmail.com/GrabServices-iOS -d Production -m",
    "ke": "adb shell input keyevent 82",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
